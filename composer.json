{
  "name": "programodo/laravel-vue-i18n-generator",
  "license": "MIT",
  "description": "A forked Version that Generates a vue-i18n compatible include file from your Laravel translations.",
  "keywords": [
    "laravel",
    "vue-i18n"
  ],
  "homepage": "http://github.com/programodo/laravel-vue-i18n-generator",
  "authors": [
    {
      "name": "Martin Lindhe",
      "email": "martin@ubique.se"
    },
    {
      "name": "Tony Murray",
      "email": "murraytony@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "ext-json": "*",
    "ext-mbstring": "*",
    "illuminate/console": "^5.0|^6.0|^7.0|^8.0|^9.0",
    "illuminate/support": "^5.0|^6.0|^7.0|^8.0|^9.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.7",
    "orchestra/testbench": "3.1.*"
  },
  "autoload": {
    "psr-4": {
      "MartinLindhe\\VueInternationalizationGenerator\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MartinLindhe\\VueInternationalizationGenerator\\GeneratorProvider"
      ]
    }
  }
}
